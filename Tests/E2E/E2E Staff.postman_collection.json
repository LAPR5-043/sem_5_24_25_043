							"});\r",
							"\r",
							"pm.test(\"Email in the first element matches the environment variable\", function () {\r",
							"    const jsonData = pm.response.json(); \r",
							"    const staffEmail = pm.environment.get(\"staffEmail\"); \r",
							"\r",
							"    pm.expect(jsonData).to.be.an('array').that.is.not.empty; \r",
							"    pm.expect(jsonData[0]).to.have.property(\"email\"); \r",
							"    pm.expect(jsonData[0].email).to.equal(staffEmail); \r",
							"});\r",
							"const jsonData = pm.response.json();\r",
							"pm.environment.set(\"staffID\", jsonData[0].staffID);\r",
							""
						],
